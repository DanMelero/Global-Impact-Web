@model Estabelecimento

<div class="d-flex">
    <div class="form-group col-8">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" />
    </div>

    <div class="form-group col-4">
        <label asp-for="Cnpj"></label>
        <input asp-for="Cnpj" class="form-control" />
    </div>
</div>

<div class="d-flex">
    <div class="form-group col-2">
        <label asp-for="Telefone"></label>
        <input asp-for="Telefone" class="form-control" />
    </div>

    <div class="form-group col-4">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>

    <div class="form-group col-3">
        <label asp-for="Senha">Senha</label>
        <input asp-for="Senha" type="password" class="form-control" />
    </div>

    <div class="form-group col-3">
        <label>Confirme a senha</label>
        <input name="senha" type="password" class="form-control" />
    </div>
</div>

<br />

<h4 class="ml-3">Endereço</h4>

<div class="d-flex">
    <div class="form-group col-3">
        <label asp-for="Endereco.Cep"></label>
        <input asp-for="Endereco.Cep" class="form-control" />
    </div>

    <div class="form-group col-4">
        <label asp-for="Endereco.Bairro"></label>
        <input asp-for="Endereco.Bairro" class="form-control" />
    </div>

    <div class="form-group col-4">
        <label asp-for="Endereco.Cidade"></label>
        <input asp-for="Endereco.Cidade" class="form-control" />
    </div>

    <div class="form-group col-1">
        <label asp-for="Endereco.UF"></label>
        <input asp-for="Endereco.UF" class="form-control" />
    </div>
</div>

<div class="d-flex">
    <div class="form-group col-6">
        <label asp-for="Endereco.Logradouro"></label>
        <input asp-for="Endereco.Logradouro" class="form-control" />
    </div>

    <div class="form-group col-2">
        <label asp-for="Endereco.Numero"></label>
        <input asp-for="Endereco.Numero" class="form-control" />
    </div>

    <div class="form-group col-4">
        <label asp-for="Endereco.Complemento"></label>
        <input asp-for="Endereco.Complemento" class="form-control" />
    </div>
</div>